<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="497.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controllers.NotePaneController">
   <children>
      <TextField fx:id="titleNotePane" editable="false" layoutX="8.0" layoutY="14.0" prefHeight="25.0" prefWidth="248.0" />
      <TextArea fx:id="textNotePane" editable="false" layoutX="7.0" layoutY="45.0" prefHeight="443.0" prefWidth="595.0" wrapText="true" />
      <Button fx:id="closeNoteButton" layoutX="578.0" layoutY="14.0" mnemonicParsing="false" onAction="#closeNotePane" text="X" />
      <Button fx:id="deleteNoteButton" layoutX="497.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleteNote" prefHeight="25.0" prefWidth="37.0" text="Del" />
      <Button fx:id="editNoteButton" layoutX="450.0" layoutY="14.0" mnemonicParsing="false" onAction="#editNote" text="Edit" />
   </children>
</AnchorPane>
